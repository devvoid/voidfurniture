{
  "multipart": [
    {
      "when": {
        "north": "false",
        "east": "false",
        "west": "false",
        "south": "false"
      },
      "apply": {
        "model": "voidfurniture:block/light_gray_fairy_light_hanging"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
          "model": "voidfurniture:block/light_gray_fairy_light",
          "uvlock": true
        }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
          "model": "voidfurniture:block/light_gray_fairy_light",
          "y": 90,
          "uvlock": true
        }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
          "model": "voidfurniture:block/light_gray_fairy_light_offset",
          "y": 180,
          "uvlock": true
        }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "voidfurniture:block/light_gray_fairy_light_offset",
          "y": 270,
          "uvlock": true
      }
    }
